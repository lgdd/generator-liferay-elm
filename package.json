{
  "name": "generator-liferay-elm",
  "version": "1.0.2",
  "description": "Yeoman generator for creating Liferay projects with Elm.",
  "author": "Louis-Guillaume Durand <louis-guillaume.durand@liferay.com>",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/lgdd/generator-liferay-elm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgdd/generator-liferay-elm"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "liferay",
    "elm",
    "liferay-js",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.31.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.0.0",
    "jest": "^25.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.0.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "yeoman-generator": "^4.0.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --runInBand"
  }
}
